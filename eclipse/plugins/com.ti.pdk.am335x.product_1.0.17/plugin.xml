<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin
    name="am335x PDK 1.0.17"
    id="com.ti.pdk.am335x.product_1.0.17"
    version="1.0.17"
    provider-name="Texas Instruments Inc.">
    <extension point="org.eclipse.help.contentProducer">
        <contentProducer>
            <producer class="org.eclipse.rtsc.xdctools.ui.help.HelpContentProducer"/>
        </contentProducer>
    </extension>

    <extension point="org.eclipse.rtsc.xdctools.ui.productInfo">
        <repository location="../../../packages"/>
        <docs location="../../../docs"/>
        <info
            installLocation="../../.."
            productName="am335x PDK"
            versionNumber="1.0.17"
        />
    </extension>


    <extension point="org.eclipse.rtsc.xdctools.managedbuild.core.rtscProductTypes">
        <productType
            id="com.ti.pdk.am335x"
            name="am335x PDK"
            folderPrefix="pdk_am335x"
            rootMacroName="TI_PDK_INSTALL_DIR"/>
    </extension>

    <extension point="org.eclipse.rtsc.xdctools.managedbuild.core.rtscProducts">
        <product
            productTypeId="com.ti.pdk.am335x"
            version="1.0.17"/>
    </extension>

</plugin>
